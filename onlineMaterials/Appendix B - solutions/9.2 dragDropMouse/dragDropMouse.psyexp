      <Param name="loopType" updates="None" val="random" valType="str"/>
      <Param name="nReps" updates="None" val="5" valType="code"/>
      <Param name="endPoints" updates="None" val="[0, 1]" valType="num"/>
      <Param name="conditions" updates="None" val="[OrderedDict([('file1', u'stims/face01.jpg'), ('file2', u'stims/face02.jpg'), ('file3', u'stims/face03.jpg')]), OrderedDict([('file1', u'stims/house01.jpg'), ('file2', u'stims/house02.jpg'), ('file3', u'stims/house03.jpg')])]" valType="str"/>
